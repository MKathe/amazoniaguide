<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cerezaconsulting.pushay">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:name="com.cerezaconsulting.pushay.PushayApplication"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.cerezaconsulting.pushay.presentation.activities.LoadActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cerezaconsulting.pushay.presentation.activities.LoginActivity">
        </activity>
        <activity android:name="com.cerezaconsulting.pushay.presentation.activities.RegisterActivity">
        </activity>
        <activity android:name="com.cerezaconsulting.pushay.presentation.activities.TicketsActivity">
        </activity>
        <activity android:name="com.cerezaconsulting.pushay.presentation.activities.ProfileActivity">
        </activity>
        <activity android:name="com.cerezaconsulting.pushay.presentation.activities.TicketsDetailActivity">
        </activity>
        <activity android:name="com.cerezaconsulting.pushay.presentation.activities.SlideActivity">
        </activity>
        <activity android:name=".presentation.activities.CountriesActivity">
        </activity>
        <activity android:name=".presentation.activities.CitiesActivity">
        </activity>
        <activity android:name=".presentation.activities.DestinyActivity">
        </activity>
        <activity android:name=".presentation.activities.ListSchedulesActivity">
        </activity>
        <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
    </application>

</manifest>